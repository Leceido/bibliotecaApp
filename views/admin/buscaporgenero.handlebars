<h1>{{genero.nome}}</h1>
{{#each livros}}
<div class="card">
    <div class="card-body">

        <h3>{{nome}}</h3>
        <p>{{sinopse}}</p>
        <small>{{genero}}</small>
        <hr>
        <small>Data de publicacao: {{data}}</small>
    </div>
</div>
{{else}}
    <h5>Nenhuma postagem</h5>
{{/each}}